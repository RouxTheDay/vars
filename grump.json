{
  "defaultCommand": "vars",
  "commands": {
    "vars": {
      "scriptType": "node",
      "scriptPath": "index.js",
      "vars": {
        "name": {
          "default": "Keith",
          "persist": true
        },
        "phrase": {
          "persist": false,
          "default": "Hello there!"
        },
        "password": {
          "persist": false,
          "hidden": true
        }
      }
    }
  }
}